<form action="/OnGoingDataCollection/update/<%= OnGoingDataCollection.id %>" method="POST" class="edit">
    <div class="form-group">
        <h2><%= __('editing a OnGoingDataCollection') %></h2>

        <div class="form-group" style="width: 400px;">
            <label><%= __('Track Session Configuration Id') %></label>
            <input value="<%= OnGoingDataCollection.TrackSessionConfigurationId %>" name="TrackSessionConfigurationId"
                   type="text" class="form-control"/>
        </div>

        <div class="form-group" style="width: 400px;">
            <label><%= __('Data Collection Time') %></label>
            <input value="<%= OnGoingDataCollection.DataCollectionTime %>" name="DataCollectionTime" type="text"
                   class="form-control"/>
        </div>

        <div class="form-group" style="width: 400px;">
            <label><%= __('Data Collection Value') %></label>
            <input value="<%= OnGoingDataCollection.DataCollectionValue %>" name="DataCollectionValue" type="text"
                   class="form-control"/>
        </div>

    </div>
    <input type="submit" value="<%= __('Update') %>" class="btn btn-default"/>
    <a href="/OnGoingDataCollection/show/<%= OnGoingDataCollection.id %>" class="btn btn-danger"><%= __('Cancel') %></a>
</form>
	
	